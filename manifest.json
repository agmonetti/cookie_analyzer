{
  "manifest_version": 3,
  "name": "Cookie Analyzer",
  "version": "2.2",
  "description": "Analiza, exporta y elimina cookies del sitio actual.",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}

